-- 10: Emergency Escapades --
-- Size Challenge: 9/9 --
-- Find your way to the emergency exit! --
-- First, make your way towards any data cube. --
-- Then, move based on what the cube says. --

-- 1: Step up --
-- 2: Step right --
-- 3: Step down --
-- 4: Step left --

main:
if c == 4 or c != datacube:
  step w
endif

if c == 1:
  step n
endif

if c == 2:
  step e
endif

if c == 3:
  step s
endif

jump main