-- 6: Little Exterminator 1 --
-- Have the little worker follow the path to the cube without falling into a hole. --

a:
b:
c:
if c == datacube:
  pickup c
endif

if s != hole:
  step s
  jump a
endif

if w != hole:
  west:
    if s != hole:
      jump b
    endif
    step w
    jump west
endif

east:
  if s != hole:
    jump c
  endif
  step e
  jump east